<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#FFFFFF</item>
        <item name="colorPrimaryDark">#FFFFFF</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="NavWhite" parent="AppTheme">
        <item name="android:statusBarColor" tools:ignore="NewApi">#F8501C</item>
        <item name="android:navigationBarColor" tools:ignore="NewApi">#FFFFFF</item>
        <item name="android:windowLightNavigationBar" tools:ignore="NewApi">true</item>
    </style>

    <style name="AllWhite" parent="AppTheme">
        <item name="android:statusBarColor" tools:ignore="NewApi">#FFFFFF</item>
        <item name="android:navigationBarColor" tools:ignore="NewApi">#FFFFFF</item>
        <item name="android:windowLightNavigationBar" tools:ignore="NewApi">true</item>
    </style>

</resources>